{
  "projectType": "library",
  "sourceRoot": "libs/main/src",
  "prefix": "ts",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/main"
      ],
      "options": {
        "jestConfig": "libs/main/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/main/**/*.ts",
          "libs/main/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
